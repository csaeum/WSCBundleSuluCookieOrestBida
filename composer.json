{
    "name": "wsc/sulu-cookie-consent-bundle",
    "description": "Sulu CMS Bundle for GDPR-compliant cookie consent management using Orest Bida Cookie Consent library",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian SÃ¤um",
            "email": "info@web-seo-consulting.eu",
            "homepage": "https://www.web-seo-consulting.eu"
        }
    ],
    "keywords": [
        "sulu",
        "cms",
        "cookie",
        "consent",
        "gdpr",
        "dsgvo",
        "orest-bida"
    ],
    "homepage": "https://github.com/csaeum/WSCBundleSuluCookieOrestBida",
    "support": {
        "issues": "https://github.com/csaeum/WSCBundleSuluCookieOrestBida/issues",
        "source": "https://github.com/csaeum/WSCBundleSuluCookieOrestBida"
    },
    "require": {
        "php": "^8.2",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.14|^3.0",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-kernel": "^7.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0|^11.0",
        "sulu/sulu": "^3.0",
        "symfony/phpunit-bridge": "^7.0"
    },
    "conflict": {
        "sulu/sulu": "<2.5"
    },
    "suggest": {
        "sulu/sulu": "Required for full Sulu CMS integration (^3.0 recommended)",
        "symfony/asset-mapper": "For modern JS asset management without build process"
    },
    "autoload": {
        "psr-4": {
            "WSC\\SuluCookieConsentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WSC\\SuluCookieConsentBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
